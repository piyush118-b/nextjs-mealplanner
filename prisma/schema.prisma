datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

model Profile {
  id                   String   @id @default(uuid())
  userId               String   @unique
  email                String

  createdAt            DateTime @default(now())
  updatedAt            DateTime @updatedAt
}
